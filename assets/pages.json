---
layout: null
---
{%- assign pages = site.pages | where_exp: "p", "p.layout != nil" -%}

{
  "pages":[
    {%- for p in pages %}
      {
        "name": {{ p.title | jsonify }},
        "url": {{ p.url | markdownify | strip_html | jsonify }}
      }
    {%- unless forloop.last %},{% endunless -%}
    {% endfor %}
  ]
}
